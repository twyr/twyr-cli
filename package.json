{
	"name": "twyr-cli",
	"description": "Generate services, middlewares, components, templates, and whole new servers confirming to the Twy'r specifications",
	"version": "0.7.1",
	"author": "Vish Desai",
	"bin": {
		"twyr-cli": "./bin/run"
	},
	"bugs": "https://github.com/twyr/twyr-cli/issues",
	"dependencies": {
		"@oclif/command": "^1.4.2",
		"@oclif/config": "^1.3.59",
		"@oclif/errors": "^1.0.2",
		"@oclif/plugin-help": "^1.1.6",
		"globby": "^8.0.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.3.1",
		"@oclif/test": "^1.0.1",
		"chai": "*",
		"docdash": "*",
		"eslint": "*",
		"eslint-config-oclif": "*",
		"eslint-plugin-jsdoc": "*",
		"grunt": "*",
		"grunt-env": "*",
		"grunt-eslint": "*",
		"grunt-exec": "*",
		"grunt-mocha-istanbul": "*",
		"grunt-mocha-test": "*",
		"istanbul": "*",
		"load-grunt-tasks": "*",
		"mocha": "*",
		"mocha-junit-reporter": "*",
		"mocha-prepare": "*"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		".oclif.manifest.json",
		"/bin",
		"/src"
	],
	"homepage": "https://github.com/twyr/twyr-cli",
	"keywords": [
		"twy'r",
		"cli"
	],
	"license": "MIT",
	"main": "src/index.js",
	"oclif": {
		"commands": "./src/commands",
		"bin": "twyr-cli",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "twyr/twyr-cli",
	"scripts": {
		"clean": "rm -f .oclif.manifest.json",
		"lint": "eslint .",
		"docs": "jsdoc --configure ./jsdoc.json",

		"prepublishOnly": "oclif-dev manifest && oclif-dev readme",
		"postpublish": "yarn run clean",

		"test": "mocha --forbid-only \"test/**/*.test.js\"",

		"version": "oclif-dev readme && git add README.md",
		"preversion": "yarn run clean"
	}
}
