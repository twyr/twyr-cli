{
	"name": "twyr-cli",
	"description": "Generate services, middlewares, components, templates, and whole new servers confirming to the Twy'r specifications",
	"version": "0.7.1",
	"homepage": "https://twyr,github.io/twyr-cli",
	"keywords": [
		"twy'r",
		"cli"
	],
	"main": "src/index.js",
	"bin": {
		"twyr-cli": "./bin/twyr-cli"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"LICENSE",
		".oclif.manifest.json",
		"README.md",
		"/bin",
		"/src"
	],
	"oclif": {
		"commands": "./src/commands",
		"bin": "twyr-cli",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-not-found"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/twyr/twyr-cli"
	},
	"author": {
		"name": "Vish Desai",
		"email": "shadyvd@hotmail.com",
		"url": "https://twyr.github.io"
	},
	"maintainers": [
		{
			"name": "Vish Desai",
			"email": "shadyvd@hotmail.com",
			"url": "https://twyr.github.io"
		}
	],
	"contributors": [
		{
			"name": "Vish Desai",
			"email": "shadyvd@hotmail.com",
			"url": "https://twyr.github.io"
		}
	],
	"bugs": {
		"url": "https://github.com/twyr/twyr-cli/issues",
		"email": "shadyvd@hotmail.com"
	},
	"scripts": {
		"build": "./node_modules/.bin/grunt --force",
		"clean": "rm -f .oclif.manifest.json && rm -rf docs && rm -rf buildresults",
		"docs": "./node_modules/.bin/jsdoc --configure ./jsdoc.json --verbose",
		"lint": "./node_modules/.bin/eslint .",
		"prepublishOnly": "oclif-dev manifest && oclif-dev readme",
		"postpublish": "npm run-script clean",
		"test": "./node_modules/.bin/mocha --forbid-only \"test/**/*.test.js\"",
		"version": "oclif-dev readme && git add README.md",
		"preversion": "npm run-script clean"
	},
	"dependencies": {
		"@oclif/command": "^1.4.33",
		"@oclif/config": "^1.6.33",
		"@oclif/errors": "^1.1.2",
		"@oclif/plugin-help": "^2.0.5",
		"@oclif/plugin-not-found": "^1.1.2",
		"globby": "^8.0.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.15.3",
		"@oclif/test": "^1.1.0",
		"chai": "*",
		"docdash2": "*",
		"eslint": "^5.0.0",
		"eslint-config-oclif": "^2.0.0",
		"eslint-junit": "*",
		"eslint-plugin-jsdoc": "^3.7.1",
		"grunt": "^1.0.3",
		"grunt-contrib-clean": "*",
		"grunt-env": "*",
		"grunt-eslint": "^21.0.0",
		"grunt-exec": "*",
		"grunt-jsbeautifier": "*",
		"grunt-mocha-istanbul": "*",
		"grunt-mocha-test": "*",
		"grunt-xmlstoke": "*",
		"istanbul": "^1.1.0-alpha.1",
		"jsdoc": "^3.0.0",
		"load-grunt-tasks": "^4.0.0",
		"mocha": "^5.2.0",
		"mocha-junit-reporter": "*",
		"mocha-prepare": "*"
	},
	"license": "MITNFA",
	"readmeFilename": "README.md"
}
